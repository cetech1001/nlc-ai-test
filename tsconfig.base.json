{
  "compilerOptions": {
    "baseUrl": ".",
    "composite": true,
    "declarationMap": true,
    "emitDeclarationOnly": true,
    "importHelpers": true,
    "isolatedModules": true,
    "lib": [
      "es2022"
    ],
    "module": "esnext",
    "moduleResolution": "bundler",
    "noEmitOnError": true,
    "noFallthroughCasesInSwitch": true,
    "noImplicitOverride": true,
    "noImplicitReturns": true,
    "noUnusedLocals": true,
    "skipLibCheck": true,
    "strict": true,
    "target": "es2022",
    "customConditions": [
      "development"
    ],
    "paths": {
      "@nlc-ai/api-auth": [
        "libs/api/auth/src/index.ts"
      ],
      "@nlc-ai/api-caching": [
        "libs/api/caching/src/index.ts"
      ],
      "@nlc-ai/api-database": [
        "libs/api/database/src/index.ts"
      ],
      "@nlc-ai/api-dto": [
        "libs/api/dto/src/index.ts"
      ],
      "@nlc-ai/api-messaging": [
        "libs/api/messaging/src/index.ts"
      ],
      "@nlc-ai/api-types": [
        "libs/api/types/src/index.ts"
      ],
      "@nlc-ai/api-validation": [
        "libs/api/validation/src/index.ts"
      ],
      "@nlc-ai/sdk-agents": [
        "libs/sdk/agents/src/index.ts"
      ],
      "@nlc-ai/sdk-analytics": [
        "libs/sdk/analytics/src/index.ts"
      ],
      "@nlc-ai/sdk-auth": [
        "libs/sdk/auth/src/index.ts"
      ],
      "@nlc-ai/sdk-billing": [
