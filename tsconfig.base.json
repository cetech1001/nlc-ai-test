      ],
      "@nlc-ai/sdk-community": [
        "libs/sdk/community/src/index.ts"
      ],
      "@nlc-ai/sdk-content": [
        "libs/sdk/content/src/index.ts"
      ],
      "@nlc-ai/sdk-core": [
        "libs/sdk/core/src/index.ts"
